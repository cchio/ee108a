 .timescale 0 0;
v0x7ff373c1b580_0 .var "clk", 0 0;
v0x7ff373c1b630_0 .var "command", 7 0;
v0x7ff373c1b6d0_0 .net "data_len", 1 0, v0x7ff373c1b500_0; 1 drivers
S_0x7ff373c0b530 .scope module, "len" "midi_data_len_lookup" 2 7, 3 1, S_0x7ff373c04940;
 .timescale 0 0;
v0x7ff373c062b0_0 .net "clk", 0 0, v0x7ff373c1b580_0; 1 drivers
v0x7ff373c1b470_0 .net "command", 7 0, v0x7ff373c1b630_0; 1 drivers
v0x7ff373c1b500_0 .var "len", 1 0;
E_0x7ff373c06500 .event posedge, v0x7ff373c062b0_0;
    .scope S_0x7ff373c0b530;
T_0 ;
    %wait E_0x7ff373c06500;
    %ix/load 1, 4, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.0, 4;
    %load/x1p 8, v0x7ff373c1b470_0, 4;
    %jmp T_0.1;
T_0.0 ;
    %mov 8, 2, 4;
T_0.1 ;
; Save base=8 wid=4 in lookaside.
    %cmpi/u 8, 8, 4;
    %jmp/1 T_0.2, 6;
    %cmpi/u 8, 9, 4;
    %jmp/1 T_0.3, 6;
    %cmpi/u 8, 10, 4;
    %jmp/1 T_0.4, 6;
    %cmpi/u 8, 11, 4;
    %jmp/1 T_0.5, 6;
    %cmpi/u 8, 12, 4;
    %jmp/1 T_0.6, 6;
    %cmpi/u 8, 13, 4;
    %jmp/1 T_0.7, 6;
    %cmpi/u 8, 14, 4;
    %jmp/1 T_0.8, 6;
    %set/v v0x7ff373c1b500_0, 0, 2;
    %jmp T_0.10;
T_0.2 ;
    %movi 8, 2, 2;
    %set/v v0x7ff373c1b500_0, 8, 2;
    %jmp T_0.10;
T_0.3 ;
    %movi 8, 2, 2;
    %set/v v0x7ff373c1b500_0, 8, 2;
    %jmp T_0.10;
T_0.4 ;
    %movi 8, 2, 2;
    %set/v v0x7ff373c1b500_0, 8, 2;
    %jmp T_0.10;
T_0.5 ;
    %movi 8, 2, 2;
    %set/v v0x7ff373c1b500_0, 8, 2;
    %jmp T_0.10;
T_0.6 ;
    %movi 8, 1, 2;
    %set/v v0x7ff373c1b500_0, 8, 2;
    %jmp T_0.10;
T_0.7 ;
    %movi 8, 1, 2;
    %set/v v0x7ff373c1b500_0, 8, 2;
    %jmp T_0.10;
T_0.8 ;
    %movi 8, 2, 2;
    %set/v v0x7ff373c1b500_0, 8, 2;
    %jmp T_0.10;
T_0.10 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x7ff373c04940;
T_1 ;
    %set/v v0x7ff373c1b580_0, 0, 1;
T_1.0 ;
    %delay 5, 0;
    %load/v 8, v0x7ff373c1b580_0, 1;
    %inv 8, 1;
    %set/v v0x7ff373c1b580_0, 8, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x7ff373c04940;
T_2 ;
    %set/v v0x7ff373c1b630_0, 0, 8;
    %vpi_call 2 20 "$monitor", "clk %b, command %h, data_len %d", v0x7ff373c1b580_0, v0x7ff373c1b630_0, v0x7ff373c1b6d0_0;
    %vpi_call 2 22 "$display", "len should equal 0";
    %delay 20, 0;
    %vpi_call 2 24 "$display", "len should equal 2";
    %movi 8, 128, 8;
    %set/v v0x7ff373c1b630_0, 8, 8;
    %delay 20, 0;
    %vpi_call 2 27 "$display", "len should equal 1";
    %movi 8, 193, 8;
    %set/v v0x7ff373c1b630_0, 8, 8;
